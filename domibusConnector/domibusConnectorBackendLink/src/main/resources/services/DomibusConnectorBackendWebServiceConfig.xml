<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:jaxws="http://cxf.apache.org/jaxws"
    xmlns:wsp="http://www.w3.org/ns/ws-policy"
    xmlns:p="http://cxf.apache.org/policy"
    xmlns:cxf="http://cxf.apache.org/core"

    xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd 
http://docs.oasis-open.org/ws-sx/ws-securitypolicy/200802 http://docs.oasis-open.org/ws-sx/ws-securitypolicy/v1.3/cd/ws-securitypolicy.xsd
http://www.w3.org/ns/ws-policy https://www.w3.org/2007/02/ws-policy.xsd
http://cxf.apache.org/policy http://cxf.apache.org/schemas/policy.xsd
http://cxf.apache.org/core http://cxf.apache.org/schemas/core.xsd "
>

    
    <cxf:bus>
        <cxf:features>
            <p:policies/>
            <cxf:logging/>
        </cxf:features>
    </cxf:bus>
    
    
    <jaxws:endpoint id="connectorBackendWS"
                    implementor="#connectorBackendImpl"

                    address="/backend"
                    xmlns:e="http://service.jaxws.cxf.apache.org/endpoint"
                    xmlns:s="http://service.jaxws.cxf.apache.org/service"
                                                           
    >
        
        <jaxws:properties>
<!--            <entry key="security.callback-handler"
                   value="interop.client.UTPasswordCallback"/>
            <entry key="security.signature.properties"
                   value="etc/keystore.properties"/>
            <entry key="security.encryption.properties"
                   value="etc/truststore.properties"/>
            <entry key="security.encryption.username"
                   value="useReqSigCert"/>-->
                   
            <entry key="ws-security.callback-handler" value-ref="myPasswordCallback"/>
            <entry key="security.callback-handler" value-ref="myPasswordCallback"/>
                   
<!--                   <entry key="ws-security.ut.validator" value="eu.domibus.connector.ws.backend.link.spring.MyUsernameTokenValidator" />-->
        </jaxws:properties>
         <jaxws:features>
            <p:policies>
                <wsp:PolicyReference URI="classpath:/wsdl/backend.policy.xml"/>
            </p:policies>
        </jaxws:features>
    </jaxws:endpoint>
        

    
    
</beans>
