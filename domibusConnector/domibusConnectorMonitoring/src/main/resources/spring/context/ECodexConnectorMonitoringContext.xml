<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	
	<bean id="monitoringDao" class="eu.domibus.connector.common.db.dao.impl.DomibusConnectorConnectorMonitoringDao">
		<property name="dataSource" ref="dataSource" />
	</bean>

	<bean id="ecodexConnectorMonitor" class="eu.ecodex.connector.monitoring.ECodexConnectorMonitor">
		<property name="monitoringDao" ref="monitoringDao"/>
	</bean>

	<bean id="ecodexConnectorMonitorController" class="eu.ecodex.connector.monitoring.ECodexConnectorMonitorController">
		<property name="connectorProperties" ref="connectorProperties"/>		
	</bean> 
	
</beans>
