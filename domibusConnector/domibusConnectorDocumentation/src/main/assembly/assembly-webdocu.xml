<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id>webdocu</id>
    <formats>
        <format>zip</format>
    </formats>
    <includeBaseDirectory>false</includeBaseDirectory>
    <fileSets>
        <fileSet>
            <includes>
                <include>*</include>
            </includes>
            <directory>${html.docu.output.directory}</directory>
            <outputDirectory>webdocu/documentation</outputDirectory>
        </fileSet>
        <fileSet>
            <includes>
                <include>*</include>
            </includes>
            <directory>${html.docu.output.directory}/../images</directory>
            <outputDirectory>webdocu/images</outputDirectory>
        </fileSet>
    </fileSets>
</assembly>
