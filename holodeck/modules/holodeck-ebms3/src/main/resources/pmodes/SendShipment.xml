<PModes>
  <PMode name="SendShipment">

    <UserService name="Customer">
      <ToPartyInfo>
        <PartyId>Customer</PartyId>
        <Role>Consumer</Role>
      </ToPartyInfo>
      <CollaborationInfo>
        <Service>SendShipment</Service>
        <Action>SendShipment</Action>
      </CollaborationInfo>
      <PayloadInfo>
        <Message label="shipMsg" maxSize="3000">
          <SoapBody />
        </Message>
      </PayloadInfo>
    </UserService>

    <Producer name="Supplier">
      <PartyId>Supplier</PartyId>
      <Role>Supplier</Role>
    </Producer>


    <Binding name="binding2">
      <MEP name="One-Way/Push">
        <Leg number="1" mpc="shipment" userService="Customer" producer="Supplier">
          <Endpoint address="http://localhost:8080/holodeck/services/msh" soapVersion="1.1" />
        </Leg>
      </MEP>
    </Binding>

  </PMode>
</PModes>