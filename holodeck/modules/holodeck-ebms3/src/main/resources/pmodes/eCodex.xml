<!-- Definition of the Buisness Process for Small Claims -->

     <UserService name="SmallClaims_Form_A">
      <ToPartyInfo>
        <PartyId>EE</PartyId>
        <Role>GW</Role>
      </ToPartyInfo>
      <CollaborationInfo>
        <Service>SmallClaims</Service>
        <Action>Form_A</Action>
      </CollaborationInfo>
      <PayloadInfo>
        <Message label="shipMsg" maxSize="3000">
          <SoapBody />
        </Message>
      </PayloadInfo>
    </UserService>

<!--Defintion of the bindings for EPO -->

    <Binding name="EE_Form_A">
      <MEP name="One-Way/Push">
	      <!-- <Leg number="1" mpc="epo" userService="EPO_Form_A" reliability="ORDER-ANON" security="sign-encrypt-body-header"> --> 
	      <Leg number="1" mpc="epo" userService="EPO_Form_A" security="sign-encrypt-body-header">
          <Endpoint address="http://localhost:8085/holodeck/services/msh" soapVersion="1.1" />
        </Leg>
      </MEP>
    </Binding>

    <Binding name="IT_Form_A">
      <MEP name="One-Way/Push">
	      <!-- <Leg number="1" mpc="epo" userService="EPO_Form_A" reliability="CALLBACK" security="sign-encrypt-body-header"> -->
	      <Leg number="1" mpc="epo" userService="EPO_Form_A" security="sign-encrypt-body-header">  
          <Endpoint address="http://localhost:8090/holodeck/services/msh" soapVersion="1.1" />
        </Leg>
      </MEP>
    </Binding>

    <Binding name="AT_Form_A">
      <MEP name="One-Way/Push">
	      <!-- <Leg number="1" mpc="epo" userService="EPO_Form_A" reliability="CALLBACK" > -->
	      <Leg number="1" mpc="epo" userService="EPO_Form_A" >  
          <Endpoint address="http://localhost:8080/holodeck/services/msh" soapVersion="1.1" />
        </Leg>
      </MEP>
    </Binding>

<!--Defintion of the bindings for Small Claims -->

    <Binding name="EE_SmallClaims_Form_A">
      <MEP name="One-Way/Push">
	      <!-- <Leg number="1" mpc="small" userService="SmallClaims_Form_A" reliability="CALLBACK" security="sign-encrypt-body-header"> --> 
	      <Leg number="1" mpc="small" userService="SmallClaims_Form_A" security="sign-encrypt-body-header">
          <Endpoint address="http://localhost:8090/holodeck/services/msh" soapVersion="1.1" />
        </Leg>
      </MEP>
    </Binding>

<!--Defintion of the PModes for EPO-->

	<PMode name="EE_Form_A" binding="EE_Form_A" /> 
	<PMode name="AT_Form_A" binding="AT_Form_A" /> 
	<PMode name="IT_Form_A" binding="IT_Form_A" /> 
	
<!--Defintion of the PModes for Small Claims-->

	<PMode name="EE_SmallClaims_Form_A" binding="EE_SmallClaims_Form_A" /> 

</PModes>
