<PModes>
	<PMode name="UploadModeA">
		<Producer name="ConsumerTestFromA">
			<PartyId type="ConsumerTestFromTypeA">ConsumerTestFromNameA</PartyId>
		</Producer>

		<UserService name="ConsumerTestServiceB">
			<ToPartyInfo>
				<PartyId type="ConsumerTestToTypeA">ConsumerTestToNameA</PartyId>
			</ToPartyInfo>

			<CollaborationInfo>
				<Service>UploadServiceA</Service>
				<Action>UploadServiceActionA</Action>
			</CollaborationInfo>
		</UserService>

		<Binding name="uploadBindingA">
			<MEP name="One-Way/Push">
				<Leg number="1" mpc="UploadMPC" soapAction="upload" reliability="CALLBACK" security="sign">
					<Endpoint address="http://localhost:8080/holodeck/services/msh" soapVersion="1.1" />
					<As4Receipt>Response</As4Receipt>
				</Leg>
			</MEP>
		</Binding>
	</PMode>

	<Producer name="ConsumerTestFromB">
		<PartyId type="ConsumerTestFromTypeB">ConsumerTestFromNameB</PartyId>
	</Producer>

	<UserService name="ConsumerTestServiceToB">
		<ToPartyInfo>
			<PartyId type="ConsumerTestToTypeB">ConsumerTestToNameB</PartyId>
		</ToPartyInfo>

		<CollaborationInfo>
			<Service>UploadServiceB</Service>
			<Action>UploadServiceActionB</Action>
		</CollaborationInfo>
	</UserService>
	
	<Binding name="uploadBindingB">
		<MEP name="One-Way/Push">
			<Leg number="1" mpc="UploadMPC" soapAction="upload" reliability="CALLBACK" security="sign"
					userService="ConsumerTestServiceToB" producer="ConsumerTestFromB">
				<Endpoint address="http://localhost:8080/holodeck/services/msh" soapVersion="1.1" />
				<As4Receipt>Response</As4Receipt>
			</Leg>
		</MEP>
	</Binding>
	
	<PMode name="UploadModeB" binding="uploadBindingB" /> 
</PModes>